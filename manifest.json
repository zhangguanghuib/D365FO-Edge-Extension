{
  "manifest_version": 3,
  "name": "D365FO Favorite Menus",
  "version": "1.0",
  "description": "D365FO Favorite Menus.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ]
}